#page-title
  %h3 Devex Products
#content-options
  #content-buttons
    = link_to 'New Product', new_product_path, :class => "awesome orange"

%table.table2
  %thead
    %tr
      %th
      %th{:scope => "col"} Paypal Product Code
      %th{:scope => "col"} Devex Product ID
      %th{:scope => "col"} Options
  %tbody
    - @products.each do |product|
      %tr
        %th{:scope => "row"}= product.name
        %td= product.paypal_product_code
        %td= product.devex_product_id
        %td
          = link_to "Edit", edit_product_path(product)
          |
          = link_to "Delete", product, :confirm => 'Are you sure?', :method => :delete

